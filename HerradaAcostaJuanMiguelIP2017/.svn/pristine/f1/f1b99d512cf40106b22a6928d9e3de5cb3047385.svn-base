package org.ip.sesion03;

public class TrianguloNumeros {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int numeroFilas = 10;
		System.out.println("Triangulo rectangulo de numeros para un valor de lado = " + numeroFilas);
		for(int i=0;i<numeroFilas;i++){
			for(int k=numeroFilas-1;k>-1;k--){
				System.out.print(" ");
				if(k <=i)
					System.out.print(k);
				else
					System.out.print(" ");
					
			}
			System.out.print("\n");
		}
		
	}
}
