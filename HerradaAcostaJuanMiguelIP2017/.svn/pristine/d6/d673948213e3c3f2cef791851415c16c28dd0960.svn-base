package org.ip.sesion02;

public class CodigoDefectuoso {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int codigo= 25000;
		if((codigo >= 14681 && codigo <= 15681) || (codigo >= 70001 && codigo <= 79999) || (codigo >= 99999 && codigo <= 110110)){
			System.out.println("El código " + codigo + " corresponde a un artiulo  defectuoso");
		}else{
			System.out.println("El código " + codigo + " corresponde a un artiulo no defectuoso");
		}
	}

}
