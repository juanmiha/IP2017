package org.ip.sesion01;

public class EnfriamientoViento {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//w = 35.74 + 0.6215t + (0.4275t - 35.75) v 0.16.
		int t = 40;
		int v = 30;
		double w = 35.74 + 0.6215*t + (0.4275*t - 35.75)*v*0.16;
		System.out.println("Temperatura = " + t + " (grados Fahrenheit)");
		System.out.println("Velocidad del viento = " + v + " (millas por hora)");
		System.out.println();
		System.out.println("Enfriamiento del viento = " + w + " (grados Fahrenheit)");
	}

}
