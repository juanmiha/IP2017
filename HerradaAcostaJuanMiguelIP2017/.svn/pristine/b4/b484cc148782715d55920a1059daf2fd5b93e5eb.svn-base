package org.ip.sesion01;

public class EnteroAleatorio {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int aleatorio = (int) Math.floor(Math.random()*-15+15);
		System.out.println("Generaremos un número aleatorio entre -15 y 15");
		System.out.println("El entero generado aleatoriamiente es: " + aleatorio );
	}

}
